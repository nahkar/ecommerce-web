{
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"singleQuote": true,
	"trailingComma": "all",
	"tabWidth": 2,
	"semi": true,
	"printWidth": 100,
	"bracketSpacing": true,
	"useTabs": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/app/(.*)$",
		"^@/assets/(.*)$",
		"^@entities/(.*)$",
		"^@features/(.*)$",
		"^@pages/(.*)$",
		"^@shared/(.*)$",
		"^@widgets/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
